{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"WarehouseIdentity": {
			"additionalProperties": false,
			"description": "The identity of the warehouse.",
			"properties": {
				"WarehouseCode": {
					"description": "The code that identifies the warehouse.",
					"type": "integer"
				},
				"WarehouseName": {
					"description": "The name of the cotton distributor.",
					"$ref": "#/definitions/nc:OrganizationName"
				},
				"WarehouseAddress": {
					"description": "The address of the warehouse.",
					"allOf": [
						{
							"$ref": "#/definitions/nc:AddressFullText"
						},
						{
							"$ref": "#/definitions/nc:LocationCountry"
						}
					]
					
				}
			},
			"required": [
				"WarehouseCode",
				"WarehouseName",
				"WarehouseAddress"
			]
		},
		"CurrencyType": {
			"additionalProperties": false,
			"description": "The type of currency this warehouse uses.",
			"properties": {
				"nc:CurrencyCode": {
					"description": "A unit of money or exchange.",
					"$ref": "#/definitions/iso_4217:CurrencyCodeType"
				}
			},
			"required": [
				"nc:CurrencyCode"
			]
		},
		"WarehouseTariffDate": {
			"additionalProperties": false,
			"description": "The begin and end date for a warehouse tariff.",
			"properties": {
				"BeginDate": {
					"description": "The effective date of the warehouse tariff.",
					"$ref": "#/definitions/nc:Date"
				},
				"EndDate": {
					"description": "The day before the effective date of the new year.",
					"$ref": "#/definitions/nc:Date"
				}
			},
			"required": [
				"BeginDate",
				"EndDate"
			]
		}
	},
	"definitions": {
		"nc:Date": {
			"namespace": "Core",
			"namespacePrefix": "nc",
			"description": "A full date.",
			"type": "string",
			"format": "date"
		},
		"nc:OrganizationName": {
			"namespace": "Core",
			"namespacePrefix": "nc",
			"description": "A name of an organization.",
			"type": "string"
		},
		"nc:AddressFullText": {
			"namespace": "Core",
			"namespacePrefix": "nc",
			"description": "A complete address.",
			"type": "string"
		},
		"nc:LocationCountry": {
			"namespace": "Core",
			"namespacePrefix": "nc",
			"description": "A country, territory, dependency, or other such geopolitical subdivision of a location.",
			"type": "string"
		},
		"iso_4217:CurrencyCodeType": {
			"description": "A data type for a currency that qualifies a monetary amount.",
			"allOf": [
			  {
				"$ref": "#/definitions/iso_4217:CurrencyCodeSimpleType"
			  }
			]
		},
		"iso_4217:CurrencyCodeSimpleType": {
			"description": "A data type for a currency that qualifies a monetary amount.",
			"enum": ["EUR","GBP","USD", "CNY"]
		}
	}
}